<?xml version="1.0" encoding="UTF-8"?>  
<odoo>
    <record id="estate_property_tag_tree" model="ir.ui.view">
        <field name="name">Estate Property Typr tree</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <tree> 
               <field name="tag_name"/>
            </tree>
        </field>
    </record>
    <record id="estate_property_tag_form" model="ir.ui.view">
        <field name="name">Estate Property Typr Form</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <form> 
              <sheet>
                    <group>
                        <p>Tag Name</p>
                        <br/>
                        <h1><field name="tag_name"/></h1>
                    </group>
              </sheet>
            </form>
        </field>
    </record>
    <record id="action_estate_property_tag" model="ir.actions.act_window">
        <field name="name">Estate Property Tag</field>
        <field name="res_model">estate.property.tag</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="estate_property_tag_menu" name="Property Tag" action="action_estate_property_tag" parent="estate_setting" ></menuitem>
</odoo>